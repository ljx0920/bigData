    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--userLoginProvider-->
    <dubbo:reference id="userLoginProvider" interface="com.dq.manage.provider.UserLoginProvider" timeout="3000"/>

    <!--dataManagerConfigureProvider-->
    <dubbo:reference id="dataManagerConfigureProvider" interface="com.dq.manage.provider.DataManagerConfigureProvider" timeout="3000"/>

    <!--upLoadClassIfyProvider-->
    <dubbo:reference id="upLoadClassIfyProvider" interface="com.dq.manage.provider.UpLoadClassIfyProvider" timeout="3000"/>

    <!--uploadFileItemProvider-->
